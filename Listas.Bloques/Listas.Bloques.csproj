<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/JosePrietoPaez/ListaBloques-Remake</RepositoryUrl>
    <AssemblyName>expanded-lists-bloques</AssemblyName>
    <RootNamespace>ExpandedLists.Blocks</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>jmpp.expanded-lists-blocks</PackageId>
    <Title>Expanded Lists/Blocks</Title>
    <Version>0.1.0</Version>
    <Authors>JosePrietoPaez</Authors>
    <Copyright>See main package</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageTags>lists;data-structures</PackageTags>
    <PackageReleaseNotes>See main package</PackageReleaseNotes>
    <Description>This package offers additional lists to Expanded Lists</Description>
    <PackageProjectUrl>See GitHub repository</PackageProjectUrl>
    <NeutralLanguage>es-ES</NeutralLanguage>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Listas\Listas.csproj" />
  </ItemGroup>

</Project>
